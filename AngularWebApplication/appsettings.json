{
  "ConnectionStrings": {
    "DefaultConnection": "Data Source=(localdb)\\MSSQLLocalDB;Initial Catalog=AngularWebApplication;Integrated Security=True; MultipleActiveResultSets=True"
  },
  "Logging": {
    "LogLevel": {
      "Default": "Warning"
    }
  },
  "AllowedHosts": "*",
  "Console": {
    "LogLevel": "Debug",
    "Default": "Information",
    "System": "Information",
    "Microsoft": "Information"
  },
  "StaticFiles": {
    "Headers": {
      "Cache-Control": "no-cache , no-store",
      "Pragma": "no-cache",
      "Expires": "-1"
    }
  }
}
